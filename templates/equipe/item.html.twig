{% trans_default_domain 'equipe' %}
<tr>
    <td class="equipe_id_cell">
        <a href="">
            {{ equipe.identifiant }}
        </a>
    </td>
    <td class="equipe_libelle_cell">
        {{ equipe.libelle }}
    </td>
    <td class="equipe_eds_cell">
        {% if equipe.isEDS %}
            <img src="{{ asset('/images/icons/valid.png') }}" alt="eds" />
        {% endif %}
    </td>
    <td class="equipe_service_cell">
        {% if equipe.equipeInterneServices|length == 0 %}
            <span>{{ t("aucun") }}</span>
            {% else %}
                <button
                        type="button"
                        class="border-0 bg-white p-0 show-services"
                        data-bs-toggle="modal"
                        :data-bs-target="'#modalDisplayServices-'"
                >
                    {{ equipe.equipeInterneServices.length }} service(s)
                </button>
        {% endif %}


    </td>
    <td class="equipe_replace_cell">
        {% if equipe.equipeInterneServices|length %}
            <button

                    data-bs-toggle="modal"
                    data-bs-target="#modalReplaceEquipe"
                    role="button"
                    style="outline: none; border: none"
                    class="bg-white"
            >
                {% include 'icons/replace.html.twig' %}
            </button>
        {% endif %}

    </td>
    <td class="equipe_checkbox_cell">
        <input
                class="checkbox_equipe form-check-input"
                value="{{ equipe.identifiant }}"
                type="checkbox"
                ref="checkbox"
        />
    </td>
    <td class="equipe_edit_cell">
        <a
                type="button"
                class="btn-update"
        >
        {% include 'icons/edit.html.twig' %}
        </a>
    </td>
    <td class="equipe_trash_cell">
        
            <button
                    type="button"
                    data-bs-toggle="modal"
                    data-bs-target="#modalDeleteOneequipe"
                    class="btn-trash"
                    data-id="{{ equipe.identifiant }}"
            >
                {% include 'icons/trash.html.twig' %}
            </button>
    </td>
</tr>
{% block javascripts %}
    <script defer type="application/javascript">

    </script>
{% endblock %}